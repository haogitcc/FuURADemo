                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding ElementName=rdbtnLicenseSingle, Path=IsChecked}" Value="False">
                                                                    <Setter Property="Text" Value="License Path :" />
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </TextBlock.Resources>
                                                </TextBlock>
                                                <TextBox Grid.Row="3" Grid.Column="1"  Grid.ColumnSpan="2" x:Name="txtLicencePath" Width="260" Margin="3" HorizontalAlignment="Left" />

                                                <DockPanel Grid.Row="4" Grid.Column="0"  Grid.ColumnSpan="3">
                                                    <Button x:Name="btnUpdate" Click="btnUpdate_Click" Width="120" Content="Update" Margin="3" HorizontalAlignment="Right" DockPanel.Dock="Right" />
                                                    <Button x:Name="btnChooseFirmware" Width="120" Content="Choose File" Margin="3" Click="btnChooseFirmware_Click" HorizontalAlignment="Right" DockPanel.Dock="Right" />
                                                </DockPanel>
                                                <StackPanel  Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Visibility="Collapsed" Name="stackPanelFrmwrUpdatePrgss" Orientation="Vertical" Margin="3">
                                                    <TextBlock x:Name="lblProgress" Text="Progress" HorizontalAlignment="Left" VerticalAlignment="Top" />
                                                    <ProgressBar  x:Name="progressBar" Width="298" Height="23" HorizontalAlignment="Left" VerticalAlignment="Top" Maximum="100">
                                                    </ProgressBar>
                                                </StackPanel>

                                                <TextBlock Grid.Row="0" Grid.Column="0" x:Name="lblSelectType" Text="Upgrade Type :" Margin="3" HorizontalAlignment="Right" FontWeight="DemiBold"/>
                                                <RadioButton Grid.Row="0" Grid.Column="1" x:Name="rdbtnFirmwareUpgrade" Content="Firmware" Margin="3" IsChecked="True" GroupName="UpgradeType" Checked="rdbtnFirmwareUpgrade_Checked"/>
                                                <RadioButton Grid.Row="0" Grid.Column="2" x:Name="rdbtnLicenceUpgrade" Content="License" Margin="3" GroupName="UpgradeType" Checked="rdbtnFirmwareUpgrade_Checked"/>
                                            </Grid>
                                        </Border>
                                    </Grid>
                                </Expander>
                                <Expander Expanded="expdrDataExtentions_Expanded" Name="expdrDataExtensions" BorderBrush="Black" BorderThickness="1" IsExpanded="False">
                                    <Expander.Header>
                                        <TextBlock Text="Data Extensions" Margin="5">
                                            <TextBlock.ToolTip>
                                                <StackPanel>
                                                    <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Data Extensions</TextBlock>
                                                    <TextBlock>
                                                        The section provides demo functionality to stream data, as a server,
                                                        <LineBreak/>
                                                        to a network port where client applications can listen and recieve tag read data.
                                                    </TextBlock>
                                                    <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                                    <WrapPanel>
                                                        <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                                        <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                                    </WrapPanel>
                                                </StackPanel>
                                            </TextBlock.ToolTip>
                                        </TextBlock>
                                    </Expander.Header>
                                    <Expander.Background>
                                        <LinearGradientBrush EndPoint="0.8,1" StartPoint="0.5,0">
                                            <GradientStop Color="#ddd" Offset="0" />
                                            <GradientStop Color="white" Offset="1" />
                                        </LinearGradientBrush>
                                    </Expander.Background>
                                    <Grid Background="White" x:Name="gridDataExtensions" IsEnabled="False">
                                        <Grid.Resources>
                                            <BooleanToVisibilityConverter x:Key="boolToVisible"/>
                                        </Grid.Resources>
                                        <Border Margin="3,1,1,1" BorderBrush="Black" BorderThickness="1">
                                            <StackPanel Margin="2" Orientation="Vertical" Name="stckPnlDataExtensions" Height="Auto">
                                                <GroupBox Header="Data Extensions" Name="grpbxDataExtensions" VerticalAlignment="Top" BorderThickness="1" BorderBrush="Black" Height="Auto">
                                                    <StackPanel Height="Auto" Name="stckpnlExtnsGrp" Width="Auto" Orientation="Vertical">
                                                        <CheckBox ToolTip="Enable Data Extensions" ToolTipService.ShowOnDisabled="True" Content="EnableDataExtensions" Height="16" HorizontalAlignment="Left" Margin="6,6,0,0" Name="chkDataExtensions" VerticalAlignment="Top" Checked="chkDataExtensions_Checked" Unchecked="chkDataExtensions_UnChecked" />
                                                        <StackPanel Name="stckpnlStreamToTcpPort" Orientation="Horizontal" Margin="2">
                                                            <RadioButton IsEnabled="False" Content="Stream to TCP port" Name="rdBtnStreamToTcpPort" GroupName="stckpnlExtnsGrp" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="20,0,0,0" MinWidth="0" Padding="4,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="135"  Checked="rdBtnStreamToTcpPort_Checked" Unchecked="rdBtnStreamToTcpPort_Checked">
                                                                <RadioButton.ToolTip>
                                                                    Streams the tag data to TCP port.
                                                                </RadioButton.ToolTip>
                                                            </RadioButton>
                                                            <TextBox MaxLength="5" Height="21" Name="txtTcpPort" Text="9055" Width="70" HorizontalAlignment="Right" VerticalAlignment="Center" FlowDirection="LeftToRight" Margin="30,0,0,0" PreviewTextInput="txtTcpPort_PreviewTextInput" TextChanged="txtTcpPort_TextChanged" PreviewLostKeyboardFocus="txtTcpPort_PreviewLostKeyboardFocus"/>
                                                        </StackPanel>
                                                        <StackPanel Name="stckpnlHttpPost" Orientation="Vertical" Margin="2">
                                                            <RadioButton IsEnabled="False" ToolTipService.ShowOnDisabled="True" GroupName="stckpnlExtnsGrp" Content=" HTTP POST" Height="16" HorizontalAlignment="Left" Margin="20,0,0,0" Name="rdBtnHttpPost" Checked="rdBtnHttpPost_Checked" Unchecked="rdBtnHttpPost_UnChecked">
                                                                <RadioButton.ToolTip>
                                                                    Enables Http Post streaming.
                                                                </RadioButton.ToolTip>
                                                            </RadioButton>
                                                            <StackPanel Visibility="{Binding ElementName=rdBtnHttpPost, Path=IsChecked, Converter={StaticResource boolToVisible}}" Name="stackPanelHttpPost" Orientation="Vertical" Margin="25,0,0,0">
                                                                <StackPanel Orientation="Horizontal" Margin="1">
                                                                    <Label Content="Reader Name" Height="27" Name="lblReaderName" Width="136" VerticalContentAlignment="Center" />
                                                                    <TextBox Height="23" Name="txtbxReaderName" Width="180" HorizontalContentAlignment="Left" VerticalContentAlignment="Top" FlowDirection="LeftToRight" Padding="1" PreviewLostKeyboardFocus="txtbxReaderName_PreviewLostKeyboardFocus"/>
                                                                </StackPanel>
                                                                <StackPanel Orientation="Horizontal" Margin="1">
                                                                    <Label Content="URL" Height="27" Name="lblWebUrl" Width="135" VerticalContentAlignment="Center" />
                                                                    <TextBox Height="23" Name="txtbxWebUrl" Width="180"/>
                                                                </StackPanel>
                                                                <StackPanel Orientation="Horizontal" Margin="1">
                                                                    <Label Content="Update Interval (sec)" Height="27" Name="lblHttpPostIntrvl" Width="135" VerticalContentAlignment="Center" />
                                                                    <TextBox Height="23" Name="txtbxHttpPostIntrvl" Width="70" Text="1" PreviewTextInput="txtbxHttpPostIntrvl_PreviewTextInput" PreviewLostKeyboardFocus="txtbxHttpPostIntrvl_PreviewLostKeyboardFocus" TextChanged="txtbxHttpPostIntrvl_TextChanged"/>
                                                                </StackPanel>
                                                            </StackPanel>
                                                        </StackPanel>
                                                    </StackPanel>
                                                </GroupBox>
                                            </StackPanel>
                                        </Border>
                                    </Grid>
                                </Expander>
                            </StackPanel>
                        </ScrollViewer>
                    </Grid>
                </Border>
                <GridSplitter BorderBrush="Black" Background="Black" BorderThickness="1" Width="1" Grid.Column="1" HorizontalAlignment="Left"/>
            </Grid>
        </Grid>
    </DockPanel>
</Window>
