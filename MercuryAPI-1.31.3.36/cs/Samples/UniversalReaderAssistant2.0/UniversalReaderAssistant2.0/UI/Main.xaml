<Window x:Class="ThingMagic.URA2.Main"        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ThingMagic.URA2" 
        xmlns:ucTagResults="clr-namespace:ThingMagic.URA2"
        xmlns:ucWriteEPC="clr-namespace:ThingMagic.URA2"
        xmlns:ucTagInspector="clr-namespace:ThingMagic.URA2"
        xmlns:ucUserMemory="clr-namespace:ThingMagic.URA2"
        xmlns:ucLockTag="clr-namespace:ThingMagic.URA2"
        xmlns:ucUntraceable="clr-namespace:ThingMagic.URA2"
        xmlns:ucAuthenticate="clr-namespace:ThingMagic.URA2"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:ucFilters="clr-namespace:ThingMagic.URA2"
        Icon="/UniversalReaderAssistant;component/Icons/jadakfaviconbg.ico" WindowStartupLocation="CenterScreen" Closing="Window_Closing" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  d:DesignHeight="768" d:DesignWidth="1024" DataContext="{Binding ElementName=tcTagResults}" WindowState="Maximized">
    <Window.CommandBindings>
        <CommandBinding Command='ApplicationCommands.Help' Executed="CommandBinding_Executed" />
    </Window.CommandBindings>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../StyleSheet/Themes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <DockPanel>
        <Grid DockPanel.Dock="Top" Name="stackPanel13" VerticalAlignment="Top" Height="48">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="0" />
                    <GradientStop Color="#C4D9EC" Offset="1" />
                </LinearGradientBrush>
            </Grid.Background>

            <StackPanel Grid.Column="0"  Orientation="Horizontal" Margin="20,0,0,0">
                <Label Margin="0,3,0,3" Content="Reader :" FontWeight="DemiBold" VerticalAlignment="Center"/>
                <Label Margin="-5,3,3,3" ToolTipService.ShowOnDisabled="True" Content="" Height="28" Name="lblReaderUri" MinWidth="145" MaxWidth="300" Foreground="Black" ToolTip="Reader URI" VerticalAlignment="Center"/>
            </StackPanel>

            <StackPanel Grid.Column="1" Orientation="Horizontal">
                <Image Name="imgReaderStatus" Width="25" Height="25" HorizontalAlignment="Center" VerticalAlignment="Center" Source="/UniversalReaderAssistant;component/Icons/LedRed.png" />
                <!--<Border Margin="0,3,0,3" VerticalAlignment="Center" x:Name="AnimatedColorButton" Width="25" Height="25" CornerRadius="25" BorderBrush="Gray" BorderThickness="1">
                    <Border.Style>
                        <Style TargetType="Border">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=lblshowStatus, Path=Content}" Value="Disconnected">
                                    <Setter Property="Background" Value="#FFE05646"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=lblshowStatus, Path=Content}" Value="Connected">
                                    <Setter Property="Background" Value="#FFE9B148"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=lblshowStatus, Path=Content}" Value="Stopped">
                                    <Setter Property="Background" Value="#FFE9B148"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=lblshowStatus, Path=Content}" Value="Reading">
                                    <Setter Property="Background" Value="Green"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=AnimatedColorButton, Path=Background}" Value="Green">
                                    <DataTrigger.EnterActions>
                                        <BeginStoryboard>
                                            <Storyboard >
                                                <DoubleAnimation
                                     Storyboard.TargetProperty="Background.Opacity"
                                     RepeatBehavior="Forever"
                                     AutoReverse="True"
                                     To="0"
                                     Duration="0:0:1.0" ></DoubleAnimation>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </DataTrigger.EnterActions>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Border.Style>
                </Border>-->
                <Label Margin="0,3,3,3" Name="lblshowStatus" Content="Disconnected" VerticalAlignment="Center" Height="30" Width="95" ToolTip="Reader Status" />
            </StackPanel>

            <StackPanel Grid.Column="2" Orientation="Horizontal">
                <Button Margin="-20,3,3,3" ToolTipService.ShowOnDisabled="True" Visibility="Hidden"  Content="Read" Name="btnRead" Click="btnRead_Click" Width="120" Height="30" IsEnabled="False" HorizontalContentAlignment="Center">
                    <Button.ToolTip>
                        Perform read based on the read behavior defined by the settings in the "Read/Write Options" sidebar.
                    </Button.ToolTip>
                </Button>
                <Button Margin="3" ToolTipService.ShowOnDisabled="True" Foreground="Black"  Content="Toggle Side Panel" Width="120" Height="30" ToolTip="Toggle Side Panel" Name="btnConnectExpander1" Click="btnConnectExpander_Click" Visibility="Collapsed"/>
                <Button Margin="3" ToolTipService.ShowOnDisabled="True" Foreground="Black"  Content="Connect" Width="120" Height="30" ToolTip="Connect To Reader" Name="btnConnectExpander" Click="btnConnectExpander_Click"/>
            </StackPanel>

            <StackPanel Grid.Column="3" Height="35" Orientation="Horizontal">
                <!--<StackPanel.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="White" Offset="0" />
                            <GradientStop Color="#C4D9EC" Offset="1" />
                        </LinearGradientBrush>
                    </StackPanel.Background>-->
                <Button Margin="3" Background="Transparent" ToolTipService.ShowOnDisabled="True" IsEnabled="False" ToolTip="Clear Tag Reads" HorizontalAlignment="Left" Name="btnClearTagReads" Click="btnClearReads_Click" VerticalAlignment="Top" >
                    <Image Name="toolbarClearTagReadsImage" Source="..\Icons\Clear.png" />
                </Button>
                <Separator/>
                <Button Margin="3" Background="Transparent" ToolTipService.ShowOnDisabled="True" IsEnabled="False" ToolTip="Save Tag Reads" HorizontalAlignment="Left" Name="saveData" VerticalAlignment="Top" Click="saveData_Click" >
                    <Image Name="toolbarSaveTagReadsImage" Source="..\Icons\Save.png" />
                </Button>
            </StackPanel>

            <StackPanel Grid.Column="4" Orientation="Horizontal">
                <Image Name="lblTemperature" Source="/UniversalReaderAssistant;component/Icons/Temperature .png" Margin="3" Height="40" Width="35" VerticalAlignment="Center" />
                <Label Name="lblReaderTemperature" Content="0&#186;C" FontWeight="Bold" Margin="-10,3,3,3" FontSize="14"  VerticalAlignment="Center"/>

                <Image Margin="3" Stretch="Fill" Width="40" Height="40" VerticalAlignment="Top" Source="/UniversalReaderAssistant;component/Icons/broadcast-icon-off.png" Name="imgTcpStreamStatus" ToolTip="Tcp streaming status" />
            </StackPanel>

            <Button Grid.Column="5" BorderBrush="Transparent" Margin="7" Background="Transparent" ToolTipService.ShowOnDisabled="True" IsEnabled="True" ToolTip="Open Help File" HorizontalAlignment="Left" Name="btnOpenHelp" VerticalAlignment="Center" Click="btnOpenHelp_Click">
                <Image Name="toolbarOpeHelpImage" Source="..\Icons\Help.png" />
            </Button>

            <Image Grid.Column="6" Margin="3,3,20,3" Name="image1" Source="/UniversalReaderAssistant;component/Images/company_logo.png" MouseDown="image1_MouseDown" HorizontalAlignment="Right"/>
        </Grid>
        <StackPanel Height="4" Grid.Row="0" Orientation="Horizontal" DockPanel.Dock="Top">
            <StackPanel.Background>
                #C4D9EC
            </StackPanel.Background>
        </StackPanel>
        <StatusBar DockPanel.Dock="Bottom" Width="Auto">
            <StatusBar.ItemsPanel>
                <ItemsPanelTemplate>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="1.8*" />
                        </Grid.ColumnDefinitions>
                    </Grid>
                </ItemsPanelTemplate>
            </StatusBar.ItemsPanel>
            <StatusBarItem Grid.Column="0" Margin="0">
                <TextBlock Height="20" Name="lblWarning" FontSize="12" TextWrapping="Wrap" VerticalAlignment="Center"/>
            </StatusBarItem>
            <StatusBarItem Grid.Column="1" Margin="0">
                <Label Content="Copyright © 2019 JADAK, a business unit of Novanta Corporation. All Rights Reserved." />
            </StatusBarItem>
        </StatusBar>
        <!-- The bar of buttons docked along the right edge: -->
        <StackPanel Name="buttonBar" Orientation="Horizontal" DockPanel.Dock="Right">
            <StackPanel.LayoutTransform>
                <RotateTransform Angle="90"/>
            </StackPanel.LayoutTransform>
            <Button Name="pane1Button" MouseEnter="pane1Button_MouseEnter" FontWeight="Normal" FontSize="13" Width="132">
                <StackPanel Orientation="Horizontal">
                    <Image Source="..\Icons\Settings.png" Stretch="None" />
                    <TextBlock Margin="5,0,0,0">Settings/Status</TextBlock>
                </StackPanel>
            </Button>
        </StackPanel>
        <!-- The Grid containing the three child Grids fills the DockPanel: -->
        <Grid Name="parentGrid" Grid.IsSharedSizeScope="True">
            <!-- Layer 0: -->
            <Grid Name="layer0" MouseEnter="layer0_MouseEnter" Width="Auto" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300*" />
                    <ColumnDefinition Width="150*" SharedSizeGroup="maincolu" />
                </Grid.ColumnDefinitions>
                <TabControl Name="tcTagResults" Background="#9CAAC1">
                    <TabItem Name="tiTagResults" GotFocus="tiTagResults_GotFocus" >
                        <TabItem.Header>
                            <TextBlock Text="Tag Results" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Tag Results (TabItem)</TextBlock>
                                        <TextBlock>
                                            This tab will display the tags read by the reader.
                                        </TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <TabItem.ContextMenu>
                            <ContextMenu  Name="ctMenu" Visibility="Collapsed">
                                <MenuItem Header="Inspect Tag" Click="MenuItem_Click" />
                                <MenuItem Header="Write EPC" Click="MenuItem_Click" />
                                <MenuItem Header="Edit User Memory" Click="MenuItem_Click" />
                                <MenuItem Header="Lock Tag" Click="MenuItem_Click"/>
                                <MenuItem Header="Untraceable" Click="MenuItem_Click"/>
                                <MenuItem Header="Authenticate" Click="MenuItem_Click"/>
                            </ContextMenu>
                        </TabItem.ContextMenu>
                        <Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding ElementName=bigNumTotalTagCounts, Path=Visibility}" Name="gridTotalTagCountsBigNum"  Margin="0,55,0,0" Panel.ZIndex="1000" Background="Transparent" Opacity=".5">
                                <Label Visibility="{Binding ElementName=bigNumTotalTagCounts, Path=Visibility}" Panel.ZIndex="1000" Margin="0,108,0,0" FontSize="32" Background="Transparent" Name="lblTotalTagCounts" Content="Total Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Visibility="Collapsed" HorizontalContentAlignment="Center" Panel.ZIndex="1000" Name="bigNumTotalTagCounts" Background="Transparent" Foreground="DarkViolet" Opacity=".5" FontSize="220" Content="{Binding ElementName=txtTotalTagReads, Path=Content}" Height="333" Margin="0,111,0,0"></Label>
                            </Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding ElementName=bigNumUniqueTagCounts, Path=Visibility}" Name="gridUniqueTagCountsBigNum" Margin="0,55,0,0"  Panel.ZIndex="1000" Background="Transparent" Opacity=".5">
                                <Label Visibility="{Binding ElementName=bigNumUniqueTagCounts, Path=Visibility}" Panel.ZIndex="1000" Margin="0,105,0,0" FontSize="32" Background="Transparent" Name="lblUniqueTagCounts" Content="Unique Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Visibility="Collapsed" HorizontalContentAlignment="Center" Panel.ZIndex="1000" Name="bigNumUniqueTagCounts" Background="Transparent" Foreground="DarkViolet" Opacity=".5" FontSize="220" Content="{Binding ElementName=totalUniqueTagsReadTextBox, Path=Content}" Height="333" Margin="0,111,0,0"></Label>
                            </Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed" Name="gridCountsBigNum"  Panel.ZIndex="1000" Background="Transparent" Opacity=".5" Margin="0,51,0,0">
                                <Label Panel.ZIndex="1000" Margin="40,0,0,20" FontSize="32" Background="Transparent" Name="gridlblUniqueTagCounts" Content="Unique Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Foreground="DarkViolet" Panel.ZIndex="1000" Margin="0,10" FontSize="120" Background="Transparent" Opacity=".8" Name="gridlblUniqueTagCountsValue" Content="{Binding ElementName=totalUniqueTagsReadTextBox, Path=Content}" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="40,150,-1,11" Panel.ZIndex="1000" FontSize="32" Background="Transparent" Name="gridlblTotalTagCounts" Content="Total Tag Count" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="0,156,-2,10" Panel.ZIndex="1000" FontSize="120" Background="Transparent" Opacity=".8" Name="gridlblTotalTagCountsValue" Content="{Binding ElementName=txtTotalTagReads, Path=Content}" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="40,284,-1,11" Panel.ZIndex="1000" FontSize="32" Background="Transparent" Name="gridlblReadRatePerSecCount" Content="Read Rate/Sec" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="0,294,-2,10" Panel.ZIndex="1000" FontSize="120" Background="Transparent" Opacity=".8" Name="gridlblReadRatePerSecCountValue" Content="{Binding ElementName=txtbxReadRatePerSec, Path=Content}" HorizontalContentAlignment="Center"></Label>
                            </Grid>
                            <ucTagResults:ucTagResults x:Name="TagResults" Width="auto" DataContext="{Binding}" />
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiTagInspector" Background="Aqua" GotFocus="tiTagInspector_GotFocus" >
                        <TabItem.Header>
                            <TextBlock Text="Tag Inspector" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Tag Inspector (TabItem)</TextBlock>
                                        <TextBlock>The Tag Inpector tab allows the full contents of a tag's memory<LineBreak />banks to be displayed in context.</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucTagInspector:ucTagInspector x:Name="TagInspector" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiWriteEPC" Background="Aqua" GotFocus="tiWriteEPC_GotFocus">
                        <TabItem.Header>
                            <TextBlock Text="Write EPC" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Write EPC (TabItem)</TextBlock>
                                        <TextBlock>The Write EPC tag operation tab allows the EPC of<LineBreak />specific tags to be updated.</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucWriteEPC:ucWriteEPC x:Name="WriteEpc" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiUserMemory" Background="Aqua" GotFocus="tiUserMemory_GotFocus" >
                        <TabItem.Header>
                            <TextBlock Text="User Memory" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">User Memory (TabItem)</TextBlock>
                                        <TextBlock>The User Memory Editor tag operation tab allows the contents of<LineBreak /> User Memory of specific tags to be viewed and updated.</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucUserMemory:UserMemory x:Name="UserMemory" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiLockTag" Background="Aqua" GotFocus="tiLockTag_GotFocus" >
                        <TabItem.Header>
                            <TextBlock Text="Lock Tag" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Lock Tag (TabItem)</TextBlock>
                                        <TextBlock>The Lock Tag operation tab allows the contents of<LineBreak /> specific tags to be locked and unlocked and<LineBreak /> the Access Password of the tag to be written</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucLockTag:ucLockTag x:Name="LockTag" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiUntraceable" Background="Aqua" GotFocus ="tiUntraceable_GotFocus">
                        <TabItem.Header>
                            <TextBlock Text="Untraceable" Margin="2">
                                <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Untraceable (TabItem)</TextBlock>
                                        <TextBlock>The "Untraceable" feature which permits obscuring all or part of the EPC;<LineBreak /> all, part, or none of the TID;<LineBreak /> and all or none of the User memory</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                                </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucUntraceable:ucUntraceable x:Name="Untraceable" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Name="tiAuthenticate" Background="Aqua" GotFocus ="tiAuthenticate_GotFocus">
                        <TabItem.Header>
                            <TextBlock Text="Authenticate" Margin="2">
                               <TextBlock.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Authenticate (TabItem)</TextBlock>
                                        <TextBlock>The “Authenticate” Tab is used to both insert and activate keys, as well as<LineBreak /> authenticate tags and obtain encrypted data using those keys.</TextBlock>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <WrapPanel>
                                            <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                            <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                        </WrapPanel>
                                    </StackPanel>
                               </TextBlock.ToolTip>
                            </TextBlock>
                        </TabItem.Header>
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucAuthenticate:ucAuthenticate x:Name="Authenticate" Width="auto" DataContext="{Binding}"/>
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
            <!-- Layer 1: -->
            <Grid Name="layer1" Visibility="Collapsed" DataContext="{Binding}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition SharedSizeGroup="column1" Width="auto"/>
                </Grid.ColumnDefinitions>
                <!-- Column 0 is empty, but column 1 contains a Grid and a GridSplitter: -->
                <Border BorderThickness="1" Grid.Column="1" CornerRadius="10">
                    <Grid Grid.Column="1" Background="#BBC4D8">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <!-- Row 0 contains a header, and row 1 contains pane-specific content: -->
                        <DockPanel Grid.Row="0">
                            <Button Background="#D9DDE7" BorderBrush="Transparent" Name="pane1Pin" Width="26" ToolTip="Auto Hide" DockPanel.Dock="Right" Click="pane1Pin_Click">
                                <Image Name="pane1PinImage" Source="..\Icons\pinHorizontal.gif"/>
                            </Button>
                            <TextBlock  Padding="8" FontWeight="Bold" TextTrimming="CharacterEllipsis" Foreground="{StaticResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" DockPanel.Dock="Left">
                            <TextBlock.Background>
                                <LinearGradientBrush>
                                        <GradientStop Color="#DCE0E8" Offset="0"/>
                                        <GradientStop Color="#D9DDE7" Offset="1"/>
                                </LinearGradientBrush>                                
                            </TextBlock.Background>                            
                            <StackPanel Orientation="Horizontal">
                                    <Image Source="..\Icons\Settings.png" Stretch="None" />
                                    <TextBlock FontSize="14" FontFamily="Segoe UI" Margin="5,0,0,0">Settings/Status</TextBlock>
                            </StackPanel>
                        </TextBlock>
                        </DockPanel>
                        <ScrollViewer Name="settingsScrollviewer"  HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" Grid.Row="1">
                            <StackPanel Grid.Row="1" Orientation="Vertical" Margin="3">
                                <Expander Collapsed="expdrConnect_Expanded" Expanded="expdrConnect_Expanded" Name="expdrConnect" BorderBrush="Black" BorderThickness="1" >
                                    <Expander.Header>
                                        <TextBlock Text="Connect" Margin="5">
                                            <TextBlock.ToolTip>
                                                <StackPanel>
                                                    <TextBlock FontWeight="Bold" FontSize="14" Margin="0,0,0,5">Connect (Type : Expander)</TextBlock>
                                                    <TextBlock>This section allows the user to select a reader and connect to it.</TextBlock>
                                                    <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                                    <WrapPanel>
                                                        <!--<Image Source="/WpfTutorialSamples;component/Images/help.png" Margin="0,0,5,0" />-->
                                                        <TextBlock FontStyle="Italic">Press F1 for more help</TextBlock>
                                                    </WrapPanel>
                                                </StackPanel>
                                            </TextBlock.ToolTip>
                                        </TextBlock>
                                    </Expander.Header>
                                    <Expander.Background>
                                        <LinearGradientBrush EndPoint="0.8,1" StartPoint="0.5,0">
                                            <GradientStop Color="#ddd" Offset="0" />
                                            <GradientStop Color="white" Offset="1" />
                                        </LinearGradientBrush>
                                    </Expander.Background>

                                    <TabControl Name="tbConnect" Height="Auto" Width="Auto" Margin="0">
                                        <TabItem Header="Connect" Name="tbconnect">
                                            <Grid Background="White" x:Name="gridConnect">
                                                <Border Margin="3,1,1,0" BorderBrush="Black" BorderThickness="1">
                                                    <Grid Margin="0,0,20,0">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="*" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.Resources>
                                                            <Style TargetType="{x:Type Label}">
